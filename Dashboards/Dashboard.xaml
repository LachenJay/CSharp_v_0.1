        <TextBlock FontSize="20" Margin="50,255,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"><Run Text="Matematika"/></TextBlock>
        <TextBlock Name="Gradescz" HorizontalAlignment="Left" Height="27" Margin="194,155,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="270"/>
        <TextBlock Name="Gradesaj" HorizontalAlignment="Left" Height="27" Margin="194,207,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="270"/>
        <TextBlock x:Name="Gradesm" HorizontalAlignment="Left" Height="27" Margin="194,260,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="270"/>
    </Grid>
</UserControl>
