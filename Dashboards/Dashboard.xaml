        <TextBlock FontSize="20" Margin="50,150,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
            Český jazyk
        </TextBlock>
        <Button Content="Zpět" HorizontalAlignment="Left" Height="57" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click"/>
